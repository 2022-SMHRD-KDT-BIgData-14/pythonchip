<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SqlSessionManager</servlet-name>
  	<display-name>SqlSessionManager</display-name>
  	<description></description>
  	<servlet-class>com.pythonch.db.SqlSessionManager</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>MapService</servlet-name>
  	<display-name>MapService</display-name>
  	<description></description>
  	<servlet-class>com.pythonchip.controller.MapService</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>FrontController</servlet-name>
  	<display-name>FrontController</display-name>
  	<description></description>
  	<servlet-class>com.pythonchip.FrontController.FrontController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SqlSessionManager</servlet-name>
  	<url-pattern>/SqlSessionManager</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MapService</servlet-name>
  	<url-pattern>/MapService</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>FrontController</servlet-name>
  	<url-pattern>/FrontController</url-pattern>
  </servlet-mapping>
</web-app>
