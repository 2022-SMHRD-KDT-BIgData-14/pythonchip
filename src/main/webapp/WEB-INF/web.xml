<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>SqlSessionManager</servlet-name>
		<display-name>SqlSessionManager</display-name>
		<description></description>
		<servlet-class>com.pythonchip.db.SqlSessionManager</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>MapService</servlet-name>
		<display-name>MapService</display-name>
		<description></description>
		<servlet-class>com.pythonchip.controller.MapService</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>FrontController</servlet-name>
		<display-name>FrontController</display-name>
		<description></description>
		<servlet-class>com.pythonchip.FrontController.FrontController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SearchStoreAjax</servlet-name>
		<display-name>SearchStoreAjax</display-name>
		<description></description>
		<servlet-class>com.pythonchip.ajax.SearchStoreAjax</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>updateService</servlet-name>
		<display-name>updateService</display-name>
		<description></description>
		<servlet-class>com.pythonchip.controller.updateService</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DeleteService</servlet-name>
		<display-name>DeleteService</display-name>
		<description></description>
		<servlet-class>com.pythonchip.controller.DeleteService</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>JoinService</servlet-name>
		<display-name>JoinService</display-name>
		<description></description>
		<servlet-class>com.pythonchip.controller.JoinService</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LoginService</servlet-name>
		<display-name>LoginService</display-name>
		<description></description>
		<servlet-class>com.pythonchip.controller.LoginService</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LogoutService</servlet-name>
		<display-name>LogoutService</display-name>
		<description></description>
		<servlet-class>com.pythonchip.controller.LogoutService</servlet-class>
	</servlet>



	<servlet-mapping>
		<servlet-name>SqlSessionManager</servlet-name>
		<url-pattern>/SqlSessionManager</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>MapService</servlet-name>
		<url-pattern>/MapService</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SearchStoreAjax</servlet-name>
		<url-pattern>/SearchStoreAjax</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>FrontController</servlet-name>
		<url-pattern>*.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>updateService</servlet-name>
		<url-pattern>/updateService</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DeleteService</servlet-name>
		<url-pattern>/DeleteService</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>JoinService</servlet-name>
		<url-pattern>/JoinService</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LoginService</servlet-name>
		<url-pattern>/LoginService</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LogoutService</servlet-name>
		<url-pattern>/LogoutService</url-pattern>
	</servlet-mapping>

</web-app>
